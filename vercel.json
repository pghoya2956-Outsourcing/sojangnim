{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "nextjs",
  "buildCommand": "next build",
  "installCommand": "npm install",
  "outputDirectory": ".next",
  "regions": ["iad1"],
  "functions": {
    "app/**/*.ts": {
      "memory": 1024,
      "maxDuration": 300
    }
  },
  "git": {
    "deploymentEnabled": false
  }
}
